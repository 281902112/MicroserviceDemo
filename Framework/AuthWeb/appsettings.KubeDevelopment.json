{
  "WebHostBuilder": {
    "UseUrls": "http://*"
  },
  "IdentityService": {
    "Authority": "http://host.docker.internal:30006"
  },
  "ApiGatewayService": {
    "Url": "http://host.docker.internal:30007"
  },
  "HostWebHybrid": {
    "RedirectUris": "http://host.docker.internal:30005/signin-oidc",
    "PostLogoutRedirectUris": "http://host.docker.internal:30005/signout-callback-oidc"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
