{
  "WebHostBuilder": {
    "UseUrls": "http://*"
  },
  "IdentityService": {
    "Authority": "http://auth-web",
    "IssuerUri": "http://localhost:30006"
  },
  "ApiGatewayService": {
    "Url": "http://api-gateway"
  },
  "HostWebHybrid": {
    "RedirectUris": "http://localhost:30005/signin-oidc",
    "PostLogoutRedirectUris": "http://localhost:30005/signout-callback-oidc"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
