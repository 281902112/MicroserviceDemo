@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1 align="center">This is a Microservice Demo.</h1>
    @if (!User.Identity.IsAuthenticated)
    {
<h3>Error! User Is not Authenticated.</h3>
    }
        <a href="/Home/Logout">Logout</a>
        <br /> <br /> <br />
        <a href="/Home/CallTestApi">CallTestApi</a>
        <br />
        <div>
           
                <dl>
                    @foreach (var item in ViewBag.CallTestApiResult)
                    {
                        <dt>@item.Key</dt>
                        <dd>@item.Value</dd>
                    }
                </dl>
            </div>

</body>
</html>
